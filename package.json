{
  "name": "hub-element-ui",
  "version": "0.0.2",
  "description": "二次封装element-ui组件库，更贴近公司业务的PC端组件库",
  "main": "dist/index.umd.js",
  "keywords": [
    "vue",
    "ui"
  ],
  "author": "xxm",
  "license": "ISC",
  "files": [
    "dist",
    "components"
  ],
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build:css": "npx gulp sass",
